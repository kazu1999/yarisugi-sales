# AWS設定
# 以下の値を実際のAWSリソースの値に置き換えてください

# Cognito設定
VITE_COGNITO_USER_POOL_ID=ap-northeast-1_xxxxxxxxx
VITE_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx

# AWSリージョン
VITE_AWS_REGION=ap-northeast-1

# API Gateway設定
VITE_API_GATEWAY_ENDPOINT=https://xxxxxxxxxx.execute-api.ap-northeast-1.amazonaws.com/dev

# DynamoDBテーブル名（オプション）
VITE_DYNAMODB_USERS_TABLE=yarisugi-users-dev
VITE_DYNAMODB_CUSTOMERS_TABLE=yarisugi-customers-dev
VITE_DYNAMODB_FAQS_TABLE=yarisugi-faqs-dev
VITE_DYNAMODB_KNOWLEDGE_TABLE=yarisugi-knowledge-dev
VITE_DYNAMODB_SALES_PROCESSES_TABLE=yarisugi-sales-processes-dev

# 開発用設定（必要に応じて）
VITE_APP_NAME=Yarisugi Sales
VITE_APP_VERSION=1.0.0